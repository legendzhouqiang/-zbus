<?xml version="1.0" encoding="UTF-8"?>
<zbus>   
	<!-- HttpProxy, capable of working in DMZ network enviornment -->
	
	<!-- Can be multiple zbus servers, for HA -->
	<!-- Token settings is for zbus authentication -->
	<httpProxy zbus="localhost:15555" connectionCount="1" consumeTimeout="10000" token="proxy">
		<proxy entry="http" token="proxy">
			<target>http://localhost:80</target> <!-- Can be multiple -->
			<!-- <target>http://localhost:8080</target>  -->
		</proxy>
		<!-- you may add multiple proxies -->
	</httpProxy> 
	
</zbus>

