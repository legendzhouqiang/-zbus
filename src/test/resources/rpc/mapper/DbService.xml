<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.zbus.rpc.biz.DbService">

	<select id="getUser" resultType="User">
		SELECT 
		  *
	    FROM 
	   	user
	   	WHERE User=#{userId}
	</select> 
	
	<select id="getProc" resultType="Map">
		SELECT * FROM proc WHERE name = #{name}
	</select> 
	
	<select id="helpCate" resultType="Map">
		SELECT * FROM help_category
	</select> 
	
</mapper>