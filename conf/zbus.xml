<?xml version="1.0" encoding="UTF-8"?>
<zbus>  

<server>
	<!-- MqServer ip, change to open IP if you are using Aliyun -->
	<ip>0.0.0.0</ip>
	<!-- MqServer port -->
	<port>15555</port>
	<verbose>false</verbose>
	<mqStore>/tmp/zbus/mq</mqStore> 
	<registerToken></registerToken>
	<!-- Support MQ filter if set to true, to support message with same keys enqueue only once -->
	<mqFitler>false</mqFitler>
	<!-- If ip is set to 0.0.0.0, the MqServer server identity IP preference -->
	<mainIpOrder></mainIpOrder>
</server> 

<zookeeper>
	<!-- The number of milliseconds of each tick -->
	<tickTime>2000</tickTime>
	<!--The number of ticks that the initial synchronization phase can take -->
	<initLimit>10</initLimit>
	<!-- The number of ticks that can pass between sending a request and getting an acknowledgement -->	 
	<syncLimit>5</syncLimit>
	<!--the directory where the snapshot is stored. do not use /tmp for storage, /tmp here is just example sakes. -->
	<dataDir>/tmp/zbus/zookeeper</dataDir>
	<!--the port at which the clients will connect -->
	<clientPort>2181</clientPort>  
	<!--the maximum number of client connections. increase this if you need to handle more clients 
	<maxClientCnxns>60</maxClientCnxns>
	--> 
</zookeeper>

<http-proxy>
	<proxy>
		<name>HttpProxyExample</name>
		<target>localhost:8080</target>
	</proxy>
</http-proxy> 

</zbus>

